---
import Layout from '../layouts/Layout.astro';
import Hero from '../components/Hero.astro';
import CheckInOut from '../components/CheckInOut.astro';
import Bienvenida from '../components/Bienvenida.astro';
import Atractivos from '../components/Atractivos.astro';
import Testimonios from '../components/Testimonios.astro';
import BlogIndex from '../components/BlogIndex.astro';


export const title = 'Inicio';
---

<Layout title={ title }>
	<Hero />
	<CheckInOut />
	<Bienvenida />
	<Atractivos />
	<Testimonios />
	<BlogIndex />
</Layout>

<script src="../assets/js/pages.js"></script>
